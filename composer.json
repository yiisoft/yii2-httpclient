{
    "name": "yiisoft/yii2-httpclient",
    "description": "HTTP client extension for the Yii framework",
    "keywords": ["yii2", "http", "httpclient", "curl"],
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/yiisoft/yii2-httpclient/issues",
        "forum": "https://www.yiiframework.com/forum/",
        "wiki": "https://www.yiiframework.com/wiki/",
        "irc": "ircs://irc.libera.chat:6697/yii",
        "source": "https://github.com/yiisoft/yii2-httpclient"
    },
    "authors": [
        {
            "name": "Paul Klimov",
            "email": "klimov.paul@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "yiisoft/yii2": "~2.0.54",
        "paragonie/random_compat": ">=1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6",
        "phpstan/phpstan": "^2.1",
        "phpstan/phpstan-phpunit": "^2.0",
        "yiisoft/yii2-debug": "^2.1",
        "yiisoft/yii2-coding-standards": "^3.0"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true
        }
    },
    "autoload": {
        "psr-4": {
            "yii\\httpclient\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    },
    "scripts": {
        "cs": "./vendor/bin/phpcs -q src tests",
        "cs-fix": "./vendor/bin/phpcbf -q src tests",
        "static": "./vendor/bin/phpstan --memory-limit=-1",
        "tests": "./vendor/bin/phpunit"
    }
}
